SubDir HAIKU_TOP src add-ons translators gif ;

SetSubDirSupportedPlatformsBeOSCompatible ;

AddResources GIFTranslator : GIFTranslator.rdef ;

Translator GIFTranslator :
	# Main Source
	GIFTranslator.cpp
	GIFView.cpp
	GIFWindow.cpp
	Prefs.cpp

	# Real Work
	GIFLoad.cpp
	LoadPalette.cpp
	GIFSave.cpp
	SavePalette.cpp
	SFHash.cpp
	:
	be translation $(TARGET_LIBSUPC++) localestub
	: true
;

DoCatalogs GIFTranslator :
	x-vnd.Haiku-GIFTranslator
	:
	GIFTranslator.cpp
	GIFView.cpp
;
